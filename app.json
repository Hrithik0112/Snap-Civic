{
  "expo": {
    "name": "SnapCivic",
    "slug": "snap-civic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appIcon.png",
    "scheme": "snapcivic",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hrithik001.snapcivic"
    },
    "android": {
      "package": "com.hrithik001.snapcivic",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appIcon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "2c3933f2-ef94-405c-9910-54c88049e225"
      }
    },
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": ["@cloudinary/url-gen", "cloudinary-react-native", "dotenv"]
      }
    }
  }
}
